<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.bcit.Leftovers.fragment.Nearby_Fragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:weightSum="1">

            <TextView
                android:id="@+id/meat_txt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:text="Meat/Protein"
                android:textSize="16dp" />

            <GridLayout
                android:layout_width="match_parent"
                android:layout_height="180dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_column="0"
                    android:layout_row="0"
                    android:orientation="horizontal">

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/beef_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/chicken_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/fish_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/pork_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="83dp"
                    android:layout_column="0"
                    android:layout_row="1"
                    android:orientation="horizontal">

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/shrimp_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/turkey_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />
                </LinearLayout>

            </GridLayout>

            <TextView
                android:id="@+id/vegetable_txt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:text="Vegetables"
                android:textSize="16dp" />

            <GridLayout
                android:layout_width="match_parent"
                android:layout_height="180dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_column="0"
                    android:layout_row="0"
