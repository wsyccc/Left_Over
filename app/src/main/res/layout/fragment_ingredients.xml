                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="83dp"
                    android:layout_column="0"
                    android:layout_row="1"
                    android:orientation="horizontal">

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/peanuts_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/rice_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />

                </LinearLayout>

            </GridLayout>

            <TextView
                android:id="@+id/dairy"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:text="Dairy"
                android:textSize="16dp" />

            <GridLayout
                android:layout_width="match_parent"
                android:layout_height="82dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="83dp"
                    android:layout_column="0"
                    android:layout_row="0"
                    android:orientation="horizontal">

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/butter_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/cheese_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/egg_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/milk_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="83dp"
                    android:layout_column="0"
                    android:layout_row="1"
                    android:orientation="horizontal">
                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/yogurt_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />
                    </LinearLayout>
            </GridLayout>

            <TextView
                android:id="@+id/fruits"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:text="Fruits"
                android:textSize="16dp" />

            <GridLayout
                android:layout_width="match_parent"
                android:layout_height="82dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="83dp"
                    android:layout_column="0"
                    android:layout_row="0"
                    android:orientation="horizontal">

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/apple_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/avocado_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/banana_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/blueberry_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="83dp"
                    android:layout_column="0"
                    android:layout_row="1"
                    android:orientation="horizontal">
                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/orange_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />
                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/mango_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />
                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/peach_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />
                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/grape_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="83dp"
                    android:layout_column="0"
                    android:layout_row="2"
                    android:orientation="horizontal">
                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/pineapple_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
