                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/asparagus_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/bellpeppers_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/bokchoy_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/broccoli_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="83dp"
                    android:layout_column="0"
                    android:layout_row="1"
                    android:orientation="horizontal">

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/cabbage_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/carrots_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/cauliflower_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/chillipepper_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="83dp"
                    android:layout_column="0"
                    android:layout_row="2"
                    android:orientation="horizontal">

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/corn_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/cucumbers_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/jalapeno_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/lemon_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="83dp"
                    android:layout_column="0"
                    android:layout_row="3"
                    android:orientation="horizontal">

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/lettuce_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/lime_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
                        app:bootstrapBrand="regular" />

                    <com.beardedhen.androidbootstrap.BootstrapCircleThumbnail
                        android:id="@+id/mushrooms_image"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_margin="10dp"
